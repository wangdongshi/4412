+/ {
+	model = "X4412 based on Exynos4412";
+	compatible = "samsung,x4412", "samsung,exynos4412";
+
+	aliases {
+		serial0 = "/serial@13800000";
+		console = "/serial@13810000";
+		mmc0 = &mshc_0;
+		mmc1 = &sdhci2;
+	};
+
+	serial@13810000 {
+		status = "okay";
+	};
+
+	ehci@12580000 {
+		compatible = "samsung,exynos-ehci";
+		reg = <0x12580000 0x100>;
+		#address-cells = <1>;
+		#size-cells = <1>;
+		phy {
